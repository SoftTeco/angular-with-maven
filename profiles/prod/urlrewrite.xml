<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
    <rule>
        <set type="response-header" name="X-Frame-Options">DENY</set>
        <set type="response-header" name="X-XSS-Protection">1</set>
        <set type="response-header" name="X-Content-Type-Options">nosniff</set>
        <set type="response-header" name="Server">Server</set>
    </rule>
</urlrewrite>
